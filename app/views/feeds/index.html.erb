<h1>Listing feeds</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Uri</th>
    <th>Title xpath</th>
  </tr>

<% @feeds.each do |feed| %>
  <tr>
    <td><%=h feed.title %></td>
    <td><%=h feed.uri %></td>
    <td><%=h feed.title_xpath %></td>
    <td><%= link_to 'Show', feed %></td>
    <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
    <td><%= link_to 'Destroy', feed, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New feed', new_feed_path %>

