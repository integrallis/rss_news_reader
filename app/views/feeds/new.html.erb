<h1>New feed</h1>

<% form_for(@feed, :url => user_feeds_url(current_user)) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :uri %><br />
    <%= f.text_field :uri %>
  </p>
  <p>
    <%= f.label :title_xpath %><br />
    <%= f.text_field :title_xpath %>
  </p>
  <p>
	<%= f.label :published %><br />
    <%= f.check_box :published %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', user_feeds_path(current_user) %>

